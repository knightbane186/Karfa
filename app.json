{
  "expo": {
    "name": "Karfa",
    "slug": "Karfa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Karfa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.knightbane186.Karfa",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Karfa to access your location."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.knightbane186.Karfa",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZGVudmVyMCIsImEiOiJjbHpxeXZidzEwd2ppMmpvbXpvdzl6NzdlIn0.hpLiowTKopstCGJbe_8gHg",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Karfa to access your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}