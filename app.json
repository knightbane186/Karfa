{
  "expo": {
    "name": "Karfa",
    "slug": "Karfa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Karfa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.knightbane186.Karfa"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.knightbane186.Karfa"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZGVudmVyMCIsImEiOiJjbHo2eTI0YnEwMzZkMmpvZXQ4MGk1OWthIn0.ak-vD94wFcHCnUuO4_h_1Q",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
